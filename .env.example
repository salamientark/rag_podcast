# Feed URL for the podcast
FEED_URL=https://example.com/podcast/feed.xml

# SQLite database URL
DATABASE_URL=sqlite:///example_dir/example_db.db

# AssemblyAI API key for transcription
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# OpenAI API key for LLM
OPENAI_API_KEY=your_openai_api_key_here

# Voyage AI API key for embeddings
VOYAGE_API_KEY=your_voyageai_api_key

# Qdrant vector database configuration
# URL for Qdrant server (default: http://localhost:6333) for local dev
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key_here
# Default collection name for podcast embeddings
QDRANT_COLLECTION_NAME=example_collection


# Cloud storage configuration (DigitalOcean Spaces or S3-compatible)
BUCKET_ENDPOINT=https://your-region.digitaloceanspaces.com
BUCKET_KEY_ID=your_access_key_id
BUCKET_ACCESS_KEY=your_secret_access_key
BUCKET_NAME=your_bucket_name
